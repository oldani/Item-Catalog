{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { 
      "extensions": [".js", ".jsx"]
    }],
    "jsx-a11y/href-no-hash": 0,
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }],
    "arrow-parens": 0,
    "no-class-assign": 0,
    "react/forbid-prop-types": [0, { "forbid": [] }]
  }
}
